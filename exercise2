"use strict";

function getResult (a,b,c){
    if (a==0) return "Ошибка";
    let d = (a, b, c) = Math.pow(b, 2) - 4 * a * c;
    let x1, x2;
    if (d > 0) {
    x1 = (-b - Math.sqrt(d))/ (2 * a);
    x2 = (-b + Math.sqrt(d))/ (2 * a);
    console.log (x1, x2);
    } else if (d === 0) {
    x1 === -b/ (2 * a);
    console.log (x1);  
    } else if (d < 0) {
    console.log ("корней нет") 
    }
}
return (2, 4, 3);